{
  "name": "loun-genie-backend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "prestart": "npm run build",
    "start": "node dist/src/index.js",
    "start:local": "cross-env ENABLE_SHUTDOWN=false NODE_ENV=development PORT=4000 node dist/src/index.js",
    "build": "tsc",
    "prisma:generate": "prisma generate",
    "seed": "npm run prisma:generate && npm run seed:raw",
    "seed:compiled": "node dist/scripts/seed.js",
    "seed:raw": "ts-node scripts/seed.ts",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "format": "prettier --write ..",
    "typecheck": "tsc --noEmit",
    "pretest": "npm run prisma:generate",
    "test": "vitest run --reporter=default --coverage",
    "coverage": "vitest run --coverage",
    "test:watch": "vitest",
    "prepare": "husky"
  },
  "dependencies": {
    "@azure/msal-node": "^3.7.4",
    "@hubspot/api-client": "^13.4.0",
    "@microsoft/microsoft-graph-client": "^3.0.7",
    "@prisma/client": "^6.16.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/express-slow-down": "^1.3.5",
    "@types/imap": "^0.8.42",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node-fetch": "^2.6.13",
    "@types/nodemailer": "^7.0.1",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.18.2",
    "express-rate-limit": "^7.5.1",
    "express-slow-down": "^3.0.0",
    "file-type": "^18.7.0",
    "helmet": "^7.2.0",
    "imap": "^0.8.19",
    "js-yaml": "^4.1.0",
    "jsonwebtoken": "^9.0.2",
    "mailparser": "^3.7.4",
    "multer": "^2.0.2",
    "node-fetch": "^3.3.2",
    "nodemailer": "^7.0.6",
    "pino": "^10.1.0",
    "pino-http": "^10.3.0",
    "pino-pretty": "^11.0.0",
    "swagger-ui-express": "^5.0.1",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0",
    "xlsx": "^0.18.5",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@types/bcrypt": "^6.0.0",
    "@types/cors": "^2.8.0",
    "@types/express": "^4.17.0",
    "@types/express-rate-limit": "^5.1.3",
    "@types/helmet": "^4.0.0",
    "@types/jest": "^30.0.0",
    "@types/js-yaml": "^4.0.9",
    "@types/mailparser": "^3.4.6",
    "@types/multer": "^1.4.12",
    "@types/node": "^20.0.0",
    "@types/supertest": "^6.0.3",
    "@types/swagger-ui-express": "^4.1.8",
    "@typescript-eslint/eslint-plugin": "^8.44.1",
    "@typescript-eslint/parser": "^8.44.1",
    "@vitest/coverage-v8": "^3.2.4",
    "cross-env": "^10.0.0",
    "eslint": "^8.57.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-security": "^3.0.1",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.1",
    "prettier": "^3.2.5",
    "prisma": "^6.16.2",
    "supertest": "^7.1.4",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.0.0",
    "vitest": "^3.2.4"
  },
  "optionalDependencies": {
    "prom-client": "^15.1.3"
  },
  "engines": {
    "node": ">=18"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "eslint --fix",
      "git add"
    ]
  }
}
