{
  "name": "loun-genie-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "npx eslint src --ext .ts,.tsx",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "coverage": "vitest run --coverage",
    "prepare": "husky",
    "start:backend": "npm run start:local --prefix ../backend",
    "seed:dev": "npm run seed --prefix ../backend -- --reset",
    "e2e:run": "node scripts/run-e2e.js",
    "e2e": "npm run -s seed:dev && npm-run-all -p start:backend e2e:run"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "eslint --fix",
      "git add"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@types/cypress": "^0.1.6",
    "@types/mocha": "^10.0.10",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@types/testing-library__react": "^10.0.1",
    "@typescript-eslint/eslint-plugin": "^8.44.1",
    "@typescript-eslint/parser": "^8.44.1",
    "@vitejs/plugin-react": "^4.7.0",
    "cypress": "^15.3.0",
    "cypress-file-upload": "^5.0.8",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.37.2",
    "husky": "^9.1.7",
    "jest-axe": "^10.0.0",
    "jsdom": "^27.0.0",
    "lint-staged": "^16.2.1",
    "rollup-plugin-visualizer": "^6.0.3",
    "start-server-and-test": "^2.0.8",
    "typescript": "^5.0.0",
    "npm-run-all": "^4.1.5",
    "vite": "^7.1.7",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "@azure/msal-browser": "^4.26.1",
    "@microsoft/microsoft-graph-client": "^3.0.7",
    "@sentry/react": "^10.14.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-toastify": "^11.0.5"
  }
}
