{
  "compilerOptions": {
    "target": "ES2020",
    "lib": ["ES2020", "DOM"],
    "isolatedModules": false,
    "types": ["cypress", "mocha"],
    "allowJs": false,
    "checkJs": false,
    "skipLibCheck": true,
    "noEmit": true,
    "declaration": false,
    "emitDeclarationOnly": false,
    "composite": false,
    "strict": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": ["../node_modules/cypress/types", "**/*.ts", "**/*.tsx"]
}
